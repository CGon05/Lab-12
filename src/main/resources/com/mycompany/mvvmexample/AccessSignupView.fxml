<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="181.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modelview.AccessSignupView">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="7.0" prefHeight="200.0" prefWidth="485.0" spacing="10.0">
         <children>
            <TextField fx:id="EmailField" alignment="TOP_LEFT" promptText="Email" />
            <TextField fx:id="nameField" alignment="TOP_LEFT" promptText="User" />
            <TextField fx:id="NumberField" promptText="number" />
            <TextField fx:id="PasswordField" promptText="password" />
            <HBox prefHeight="32.0" prefWidth="465.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#regRecord" text="Signup" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#switchToSecondary" text="Login" textAlignment="CENTER" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
